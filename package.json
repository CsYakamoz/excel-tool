{
    "name": "@csyakamoz/excel-tool",
    "version": "0.1.1",
    "description": "My tools for parsing Excel",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CsYakamoz/excel-tool.git"
    },
    "author": "CsYakamoz",
    "license": "MIT",
    "scripts": {
        "clean": "rimraf dist",
        "build": "tsc -p tsconfig.json",
        "test": "jest",
        "prepare": "npm-run-all clean build",
        "prepublishOnly": "npm-run-all test lint",
        "lint": "eslint src --ext .ts"
    },
    "devDependencies": {
        "@types/jest": "^25.2.3",
        "@typescript-eslint/eslint-plugin": "^3.0.0",
        "@typescript-eslint/parser": "^3.0.0",
        "eslint": "^7.0.0",
        "jest": "^26.0.1",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.0.0",
        "typescript": "^3.9.2"
    },
    "dependencies": {
        "xlsx": "^0.16.1"
    },
    "keywords": [
        "excel",
        "tool"
    ],
    "bugs": {
        "url": "https://github.com/CsYakamoz/excel-tool/issues"
    },
    "homepage": "https://github.com/CsYakamoz/excel-tool#readme",
    "files": [
        "dist/**/*"
    ],
    "prettier": {
        "useTabs": false,
        "tabWidth": 4,
        "singleQuote": true,
        "semi": true,
        "arrowParens": "always",
        "trailingComma": "es5",
        "printWidth": 80,
        "endOfLine": "lf"
    },
    "jest": {
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "testRegex": "src/__tests__/.*\\.test\\.ts$"
    }
}
